{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "plugins": [
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
       "no-underscore-dangle": 0,
       "no-tabs": 0,
       "no-return-await": 0,
       "indent":["error", "tab"]
    }
}